{
  "name": "react-console-emulator",
  "version": "2.0.0",
  "description": "A simple terminal emulator component for React. ",
  "main": "lib/components/Terminal.js",
  "files": [
    "lib/utils/**"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack -p",
    "compile": "babel src --ignore \"src/**/*.scss\",\"src/**/*.html\",\"src/App.jsx\",\"src/index.jsx\" --out-dir lib",
    "lint": "eslint .",
    "codecov": "codecov --disable=gcov",
    "test": "npm run lint && npm run test-coverage && cross-env PROD_RUN=true npm run test-nocoverage && npm run codecov",
    "test-local": "npm run lint && npm run test-coverage",
    "test-dev": "jest --detectOpenHandles --watch --config jest.default.js",
    "test-coverage": "jest --detectOpenHandles --config jest.coverage.js",
    "test-nocoverage": "jest --detectOpenHandles --config jest.default.js",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-rcon/react-console-emulator.git"
  },
  "keywords": [
    "react",
    "console",
    "react-component",
    "terminal"
  ],
  "author": "Linus Willner <hello@linuswillner.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-rcon/react-console-emulator/issues"
  },
  "homepage": "https://github.com/js-rcon/react-console-emulator#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "~7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.7.0",
    "jsdom": "^13.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "puppeteer": "^1.11.0",
    "react": "^16.6.3",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.2",
    "sass-loader": "^7.1.0",
    "skip-if": "^1.1.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react-inner-html": "^1.0.1",
    "stringify-object": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
