{
  "name": "react-console-emulator",
  "version": "3.0.5",
  "description": "A simple terminal emulator component for React. ",
  "main": "dist/Terminal.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack -p",
    "compile": "babel lib --out-dir dist",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "codecov": "codecov --disable=gcov",
    "test": "npm run lint && npm run test-coverage && cross-env PROD_RUN=true npm run test-nocoverage && npm run codecov",
    "test-local": "npm run lint && npm run test-coverage",
    "test-coverage": "jest --config jest.coverage.js",
    "test-nocoverage": "jest --config jest.default.js",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-rcon/react-console-emulator.git"
  },
  "keywords": [
    "react",
    "console",
    "react-component",
    "terminal"
  ],
  "author": "Linus Willner <hello@linuswillner.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-rcon/react-console-emulator/issues"
  },
  "homepage": "https://github.com/js-rcon/react-console-emulator#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-runtime": "^6.26.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.2.1",
    "jest": "^25.4.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom": "^16.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "puppeteer": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "sass-loader": "^8.0.2",
    "skip-if": "^1.1.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "defaults": "^1.0.3",
    "prop-types": "^15.7.2",
    "react-fast-compare": "^3.0.1",
    "react-inner-html": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
