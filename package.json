{
  "name": "react-console-emulator",
  "version": "3.0.4",
  "description": "A simple terminal emulator component for React. ",
  "main": "dist/Terminal.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack -p",
    "compile": "babel lib --out-dir dist",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "codecov": "codecov --disable=gcov",
    "test": "npm run lint && npm run test-coverage && cross-env PROD_RUN=true npm run test-nocoverage && npm run codecov",
    "test-local": "npm run lint && npm run test-coverage",
    "test-coverage": "jest --config jest.coverage.js",
    "test-nocoverage": "jest --config jest.default.js",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-rcon/react-console-emulator.git"
  },
  "keywords": [
    "react",
    "console",
    "react-component",
    "terminal"
  ],
  "author": "Linus Willner <hello@linuswillner.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-rcon/react-console-emulator/issues"
  },
  "homepage": "https://github.com/js-rcon/react-console-emulator#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom": "^15.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "puppeteer": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "sass-loader": "^8.0.1",
    "skip-if": "^1.1.1",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "defaults": "^1.0.3",
    "prop-types": "^15.7.2",
    "react-fast-compare": "^2.0.4",
    "react-inner-html": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
